{
  "name": "hedali/phpsocket.io",
  "description": "A server side alternative implementation of socket.io in PHP based on Workerman, Support Workerman V5 And PHP 8.2",
  "type": "library",
  "keywords": [
    "socket.io",
    "phpsocket.io",
    "workerman",
    "sockets",
    "async",
    "stream",
    "server",
    "non-blocking"
  ],
  "homepage": "https://www.workerman.net",
  "license": "MIT",
  "require": {
    "workerman/workerman": "4.*|5.*",
    "workerman/channel": ">=1.0.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "PHPSocketIO\\": "./src"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7"
  }
}
